{
  "name": "holland-codes-personality-test-api",
  "version": "1.0.0",
  "description": "Holland Codes Personality Test HTTP API using Node.js, Express.js and Knex.js",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/index.js",
    "server:prod": "node ./dist/index.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratyaman/holland-codes-personality-test-api.git"
  },
  "keywords": [
    "Holland",
    "Codes",
    "Personality",
    "Test",
    "HTTP",
    "API"
  ],
  "author": "Haci Murat Yaman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muratyaman/holland-codes-personality-test-api/issues"
  },
  "homepage": "https://github.com/muratyaman/holland-codes-personality-test-api#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.20.7",
    "morgan": "^1.9.1",
    "mssql": "^6.0.1",
    "mysql": "^2.17.1",
    "pg": "^7.17.1",
    "sqlite3": "^4.1.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "nodemon": "^2.0.2"
  }
}
